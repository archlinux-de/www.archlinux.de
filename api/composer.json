{
    "type": "project",
    "name": "archlinux-de/www.archlinux.de",
    "description": "The German Arch Linux website",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^8.0",
        "ext-FFI": "*",
        "ext-SimpleXML": "*",
        "ext-apcu": "^5.1",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-pdo_mysql": "*",
        "doctrine/doctrine-bundle": "^2.5.4",
        "doctrine/doctrine-migrations-bundle": "^3.2.1",
        "doctrine/orm": "^2.10.4",
        "elasticsearch/elasticsearch": "^7.16.0",
        "ezyang/htmlpurifier": "^4.14.0",
        "league/iso3166": "^4.0.0",
        "maxmind-db/reader": "^1.11.0",
        "sensio/framework-extra-bundle": "^6.2.4",
        "symfony/console": "^6.0.2",
        "symfony/dotenv": "^6.0.2",
        "symfony/expression-language": "^6.0.1",
        "symfony/flex": "^v2.0.1",
        "symfony/framework-bundle": "^6.0.2",
        "symfony/http-client": "^6.0.2",
        "symfony/lock": "^6.0.2",
        "symfony/monolog-bundle": "^3.7.1",
        "symfony/property-access": "^6.0.2",
        "symfony/runtime": "^6.0.0",
        "symfony/serializer": "^6.0.2",
        "symfony/string": "^6.0.2",
        "symfony/twig-bundle": "^6.0.1",
        "symfony/validator": "^6.0.2",
        "symfony/yaml": "^6.0.2"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "malukenho/mcbumpface": "^1.1.5",
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan": "^1.3.0",
        "phpstan/phpstan-deprecation-rules": "^1.0.0",
        "phpstan/phpstan-doctrine": "^1.0.2",
        "phpstan/phpstan-phpunit": "^1.0.0",
        "phpstan/phpstan-symfony": "^1.0.4",
        "phpunit/phpunit": "^9.5.11",
        "pierres/doctrine-migrations-test": "^1.4.2",
        "pierres/symfony-database-test": "^1.2.2",
        "squizlabs/php_codesniffer": "^3.6.2",
        "symfony/phpunit-bridge": "^6.0.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpstan/extension-installer": true,
            "malukenho/mcbumpface": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*"
        },
        "mc-bumpface": {
            "stripVersionPrefixes": true
        }
    }
}
